##quick 1 minute script to grab when PWs were changed and adding ## of days to forecast next time pws are changed. output is poor quality
$passw = Get-ADUser -Server $server -Filter * -Properties PasswordLastSet, PasswordNeverExpires `
    | Where-Object {($_.PasswordLastSet -NE $null) `
        -and ($_.PasswordNeverExpires -NE $true)} `
    | Select-Object -Property PasswordLastSet 
    #| Export-Csv -Path C:\PSChanges.csv -NoTypeInformation

$futurePW = foreach ($pass in $passw.passwordlastset){
 $newdate = ($pass).adddays(##)
}
$futurePW | Export-Csv -Path C:\PSChanges.csv -NoTypeInformation
